{
  "manifest_version": 2,

  "name": "Feels",
  "description": "This extension performs Emotion Analysis on text content of current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Feels"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}